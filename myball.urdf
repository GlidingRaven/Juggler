<?xml version="1.0" ?>
<robot name="urdf_robot">
  <link name="base_link">

    <contact>
      <rolling_friction value="0.001"/>
      <spinning_friction value="0.001"/>
      <restitution value="0.8"/>
    </contact>


    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="0.0"/>
       <inertia ixx="1" ixy="1" ixz="1" iyy="1" iyz="1" izz="1"/>
    </inertial>
    

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	       <sphere radius="0.02"/>
      </geometry>
    </collision>


  </link>
</robot>